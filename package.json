{
  "name": "dotenv-ctrl",
  "version": "0.1.1",
  "description": "A CLI tool to control dotenv file",
  "keywords": [
    "dotenv"
  ],
  "homepage": "https://github.com/p-chan/dotenv-ctrl#readme",
  "bugs": "https://github.com/p-chan/dotenv-ctrl/issues",
  "license": "MIT",
  "author": "P-Chan",
  "files": [
    "dist"
  ],
  "bin": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/p-chan/dotenv-ctrl.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "dev": "ts-node ./src/index.ts",
    "fix": "prettier --write .",
    "lint": "prettier --check .",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "vitest",
    "version:major": "standard-version -r major",
    "version:minor": "standard-version -r minor",
    "version:patch": "standard-version -r patch"
  },
  "dependencies": {
    "cac": "6.7.14",
    "dotenv": "16.4.7"
  },
  "devDependencies": {
    "@stardust-configs/prettier-config": "0.3.0",
    "@stardust-configs/tsconfig": "0.4.0",
    "@types/node": "20.16.10",
    "@types/rimraf": "4.0.5",
    "@types/shelljs": "0.8.15",
    "prettier": "3.3.3",
    "rimraf": "6.0.1",
    "shelljs": "0.8.5",
    "standard-version": "9.5.0",
    "tsx": "4.19.2",
    "typescript": "5.6.2",
    "vitest": "2.1.8"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write ./CHANGELOG.md"
    }
  }
}
