{
  "name": "dotenv-ctrl",
  "version": "0.1.1",
  "description": "A CLI tool to control dotenv file",
  "keywords": [
    "dotenv"
  ],
  "homepage": "https://github.com/p-chan/dotenv-ctrl#readme",
  "bugs": "https://github.com/p-chan/dotenv-ctrl/issues",
  "license": "MIT",
  "author": "P-Chan",
  "files": [
    "dist"
  ],
  "bin": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/p-chan/dotenv-ctrl.git"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./dist",
    "dev": "ts-node ./src/index.ts",
    "prebuild": "npm run clean",
    "prepublishOnly": "npm run build",
    "test": "jest",
    "version:major": "standard-version -r major",
    "version:minor": "standard-version -r minor",
    "version:patch": "standard-version -r patch"
  },
  "dependencies": {
    "cac": "^6.7.1",
    "dotenv": "^8.2.0"
  },
  "devDependencies": {
    "@stardust-configs/prettier-config": "0.1.1",
    "@stardust-configs/tsconfig": "0.1.0",
    "@types/jest": "26.0.23",
    "@types/node": "14.17.1",
    "@types/rimraf": "3.0.0",
    "@types/shelljs": "0.8.8",
    "jest": "26.6.3",
    "prettier": "2.3.0",
    "rimraf": "3.0.2",
    "shelljs": "0.8.4",
    "standard-version": "9.3.0",
    "ts-jest": "26.5.6",
    "ts-node": "9.1.1",
    "typescript": "4.3.2"
  }
}
